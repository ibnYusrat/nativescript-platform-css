{
  "name": "nativescript-platform-css-free",
  "version": "1.6.14",
  "description": "A NativeScript plugin to deal with Declarative UI and Platform CSS. Since the original plugin requires a subscription now, this plugin is my attempt to maintain it to the point where it does not break.",
  "main": "platformcss",
  "nativescript": {
    "platforms": {
      "android": "6.0.0",
      "ios": "6.0.0"
    },
    "plugin": {
      "nan": "false",
      "pan": "true",
      "core3": "true",
      "category": "Processing"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibnYusrat/nativescript-platform-css.git"
  },
  "dependencies": {
    "nativescript-globalevents-free": "latest",
    "nativescript-platform-free": "latest"
  },
  "keywords": [
    "NativeScript",
    "platform",
    "css",
    "ios",
    "android",
    "notch",
    "softnav"
  ],
  "author": {
    "name": "Nathanael Anderson",
    "email": "nathan@master-technology.com"
  },
